{
  "name": "my-react-boilerplate",
  "version": "1.0.0",
  "description": "Starter package for React/React-Router/Redux/Express",
  "scripts": {
    "clean": "rimraf app/public/build",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node server_es6.js",
    "lint": "eslint src",
    "test": "NODE_ENV=production mocha --require ./test/setup.js test/**/*.test.js",
    "test:watch": "npm test -- --watch"
  },
